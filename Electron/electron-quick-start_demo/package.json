{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "app/main.js",
  "scripts": {
    "start": "concurrently \"npm run start_render\" \"wait-on http://localhost:4000 && npm run start_main\"",
    "start_main": "electron .",
    "start_render": "cd renderer && npm run serve",
    "build_win32": "npm run build_renderer && npm run build_main_win32",
    "build_main_win32": "cross-env NPM_CONFIG_ELECTRON_MIRROR=http://npm.taobao.org/mirrors/electron/ electron-builder build --win --ia32",
    "build_renderer": "cd renderer && npm run build"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "electron": "^8.0.3",
    "electron-builder": "^22.4.1",
    "wait-on": "^4.0.2"
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "electron-is-dev": "^1.2.0"
  }
}
